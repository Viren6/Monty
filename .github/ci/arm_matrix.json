{
  "config": [
    {
      "name": "Android NDK aarch64",
      "os": "ubuntu-22.04",
      "simple_name": "android",
      "compiler": "rustc",
      "target": "aarch64-linux-android",
      "emu": "qemu-aarch64",
      "comp": "ndk",
      "shell": "bash",
      "archive_ext": "tar"
    },
    {
      "name": "Android NDK arm",
      "os": "ubuntu-22.04",
      "simple_name": "android",
      "compiler": "rustc",
      "target": "armv7-linux-androideabi",
      "emu": "qemu-arm",
      "comp": "ndk",
      "shell": "bash",
      "archive_ext": "tar"
    }
  ],
  "binaries": [ "armv8-dotprod", "armv8", "armv7", "armv7-neon" ],
  "exclude": [
    {
      "binaries": "armv8-dotprod",
      "config": {
        "target": "armv7-linux-androideabi"
      }
    },
    {
      "binaries": "armv8",
      "config": {
        "target": "armv7-linux-androideabi"
      }
    },
    {
      "binaries": "armv7",
      "config": {
        "target": "aarch64-linux-android"
      }
    },
    {
      "binaries": "armv7-neon",
      "config": {
        "target": "aarch64-linux-android"
      }
    }
  ]
}
